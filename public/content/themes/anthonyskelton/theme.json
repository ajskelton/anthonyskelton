{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"palette": [
				{
					"name": "Background",
					"slug": "background",
					"color": "#F0F0F0"
				},
				{
					"name": "Foreground",
					"slug": "foreground",
					"color": "#181A1F"
				},
				{
					"name": "Primary Light",
					"slug": "primary-light",
					"color": "#a0c2ef"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#4185de"
				},
				{
					"name": "Primary Dark",
					"slug": "primary-dark",
					"color": "#21436f"
				},
				{
					"name": "Secondary Light",
					"slug": "secondary-light",
					"color": "#efa0c2"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#DE4185"
				},
				{
					"name": "Secondary Dark",
					"slug": "secondary-dark",
					"color": "#6f2143"
				},
				{
					"name": "Tertiary Light",
					"slug": "tertiary-light",
					"color": "#c2efa0"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#85DE41"
				},
				{
					"name": "Tertiary Dark",
					"slug": "tertiary-dark",
					"color": "#436f21"
				},
				{
					"name": "Code Background",
					"slug": "code-background",
					"color": "#292e38"
				}
			]
		},
		"custom": {
			"blockGap": "1.5rem"
		},
		"layout": {
			"contentSize": "680px",
			"wideSize": "1200px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Pixel - Fixed (1px)",
					"size": "1px",
					"slug": "px"
				},
				{
					"name": "Step 1 - Fixed (0.25rem)",
					"size": "0.25rem",
					"slug": "10"
				},
				{
					"name": "Step 2 - Fixed (0.5rem)",
					"size": "0.5rem",
					"slug": "20"
				},
				{
					"name": "Step 3 - Fixed (0.75rem)",
					"size": "0.75rem",
					"slug": "30"
				},
				{
					"name": "Step 4 - Fixed (1rem)",
					"size": "1rem",
					"slug": "40"
				},
				{
					"name": "Step 5 - Fixed (1.25rem)",
					"size": "1.25rem",
					"slug": "50"
				},
				{
					"name": "Step 6 - Fixed (1.5rem)",
					"size": "1.5rem",
					"slug": "60"
				},
				{
					"name": "Step 7 - Fixed (1.75rem)",
					"size": "1.75rem",
					"slug": "70"
				},
				{
					"name": "Step 8 - Fixed (2rem)",
					"size": "2rem",
					"slug": "80"
				},
				{
					"name": "Step 9 - Fluid (2.25rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 1.0417)), 2.25rem)",
					"slug": "90"
				},
				{
					"name": "Step 10 - Fluid (2.5rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 2.0833)), 2.5rem)",
					"slug": "100"
				},
				{
					"name": "Step 12 - Fluid (3rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 4.1667)), 3rem)",
					"slug": "120"
				},
				{
					"name": "Step 16 - Fluid (4rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 8.3333)), 4rem)",
					"slug": "160"
				},
				{
					"name": "Step 20 - Fluid (5rem)",
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 8.3333)), 5rem)",
					"slug": "200"
				},
				{
					"name": "Step 24 - Fluid (6rem)",
					"size": "clamp(4rem, calc(4rem + ((1vw - 0.4rem) * 8.3333)), 6rem)",
					"slug": "240"
				},
				{
					"name": "Step 32 - Fluid (8rem)",
					"size": "clamp(4rem, calc(4rem + ((1vw - 0.4rem) * 16.6667)), 8rem)",
					"slug": "320"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./fonts/atkinson-hyperlegible-v11-latin-regular.woff2"
							]
						},
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./fonts/atkinson-hyperlegible-v11-latin-700.woff2"
							]
						},
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./fonts/atkinson-hyperlegible-v11-latin-italic.woff2"
							]
						},
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./fonts/atkinson-hyperlegible-v11-latin-700italic.woff2"
							]
						}
					],
					"fontFamily": "\"Atkinson Hyperlegible\", sans-serif",
					"name": "Atkinson Hyperlegible",
					"slug": "atkinson-hyperlegible"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.94rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "sm",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "paragraph",
					"name": "Paragraph"
				},
				{
					"fluid": {
						"min": "1.35rem",
						"max": "1.56rem"
					},
					"size": "1.56rem",
					"slug": "h6",
					"name": "H6"
				},
				{
					"fluid": {
						"min": "1.62rem",
						"max": "1.95rem"
					},
					"size": "1.95rem",
					"slug": "h5",
					"name": "H5"
				},
				{
					"fluid": {
						"min": "1.94rem",
						"max": "2.44rem"
					},
					"size": "2.44rem",
					"slug": "h4",
					"name": "H4"
				},
				{
					"fluid": {
						"min": "2.0rem",
						"max": "3.05rem"
					},
					"size": "3.05rem",
					"slug": "h3",
					"name": "H3"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "3.82rem"
					},
					"size": "3.82rem",
					"slug": "h2",
					"name": "H2"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "4.77rem"
					},
					"size": "4.77rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": {
						"min": "2.50rem",
						"max": "5.96rem"
					},
					"size": "5.96rem",
					"slug": "h1",
					"name": "H1"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/embed": {
				"border": {
					"radius": "10px",
					"width": "2px",
					"color": "var:preset|color|foreground",
					"style": "solid"
				},
				"css": "overflow: hidden; & iframe { width: 100%; }"
			},
			"core/image": {
				"border": {
					"radius": "10px",
					"width": "2px",
					"color": "var:preset|color|foreground",
					"style": "solid"
				}
			},
			"core/paragraph": {
				"css": "> code { background-color: var(--wp--preset--color--code-background); color: var(--wp--preset--color--background); padding: 0.1em 0.3em; border-radius: 0.2em; }"
			},
			"core/quote": {
				"border": {
					"width": "4px",
					"color": "var:preset|color|primary",
					"style": "solid",
					"radius": "10px"
				},
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|40",
						"right": "var:preset|spacing|40",
						"bottom": "var:preset|spacing|40",
						"left": "var:preset|spacing|40"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|h-4",
					"lineHeight": "1.3",
					"fontStyle": "italic"
				},
				"variations": {
					"plain": {
						"border": {
							"width": "0px"
						}
					},
					"test": {
						"border": {
							"width": "0px"
						},
						"typography": {
							"fontSize": "var:preset|font-size|paragraph"
						}
					}
				}
			}
		},
		"color": {
			"background": "var:preset|color|background",
			"text": "var:preset|color|foreground"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0px",
					"color": "var:preset|color|background",
					"width": "2px"
				},
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|20",
						"right": "var:preset|spacing|50",
						"bottom": "var:preset|spacing|20",
						"left": "var:preset|spacing|50"
					}
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|background"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|secondary",
						"text": "var:preset|color|background"
					}
				},
				":focus": {
					"color": {
						"background": "var:preset|color|secondary",
						"text": "var:preset|color|background"
					},
					"border": {
						"color": "var:preset|color|background"
					}
				},
				":active": {
					"color": {
						"background": "var:preset|color|background",
						"text": "var:preset|color|secondary"
					}
				},
				":visited": {
					"color": {
						"text": "var:preset|color|background"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|h-1"
				}
			},
			"h2": {
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|80"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|h-2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|h-3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|h-4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|h-5"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|h-6"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|foreground"
					},
					"typography": {
						"textDecoration": "underline var(--wp--preset--color--foreground)"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var:preset|color|foreground"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var:preset|color|primary-dark"
				},
				"typography": {
					"textDecoration": "underline var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "var:custom|block-gap",
			"padding": {
				"top": "var:preset|spacing|30",
				"right": "var:preset|spacing|60",
				"bottom": "var:preset|spacing|30",
				"left": "var:preset|spacing|60"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|atkinson-hyperlegible",
			"fontSize": "var:preset|font-size|paragraph",
			"lineHeight": "1.6"
		}
	},
	"customTemplates": [
		{
			"name": "page-no-title",
			"title": "Page (No Title)"
		}
	],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer Credit Area",
			"area": "footer"
		},
		{
			"name": "comments",
			"title": "Comments",
			"area": "uncategorized"
		},
		{
			"name": "post-meta",
			"title": "Post Meta",
			"area": "uncategorized"
		}
	],
	"version": 3
}
